{
  "websites": [
    {
      "allowed_domains": "startitup.sk",
      "start_urls": [
        "https://www.startitup.sk/"
      ],
      "link_selector": [
        "//article//a/@href"
      ],
      "title_selector": "//h1/text()",
      "content_selectors": [
        "//div[contains(@class, 'article__main')]//p/text()"
      ]
    },
    {
      "allowed_domains": "dennikn.sk",
      "start_urls": [
        "https://dennikn.sk/blog/"
      ],
      "link_selector": [
        "//h3//a/@href"
      ],
      "title_selector": "//h1//text()",
      "content_selectors": [
        "//div[contains(@class, 'entry-content')]//p/text()"
      ],
      "pagination_link": "//nav//a[contains(text(), 'Star≈°ie')]/@href",
      "page_limit": 5
    },
    {
      "allowed_domains": "emefka.sk",
      "start_urls": [
        "https://emefka.sk/"
      ],
      "link_selector": [
        "//h3[contains(@class, 'entry-title')]//a/@href",
        "//h2[contains(@class, 'entry-title')]//a/@href"
      ],
      "title_selector": "//h1/text()",
      "content_selectors": [
        "//div[@itemprop='articleBody']//p/text()"
      ]
    },
    {
      "allowed_domains": "hnonline.sk",
      "start_urls": [
        "https://hnonline.sk/"
      ],
      "link_selector": [
        "//a[contains(@class, 'card__article-link')]/@href"
      ],
      "title_selector": "//h1[contains(@class, 'item__title')]/text()",
      "content_selectors": [
        "//div[@class='itemFullText']//p/text()"
      ]
    },
     {
      "allowed_domains": "www.zoznam.sk",
      "start_urls": [
        "https://urobsisam.zoznam.sk/",
        "https://sportky.zoznam.sk/",
        "https://openiazoch.zoznam.sk/",
        "https://dromedar.zoznam.sk/"
      ],
      "link_selector": [
        "//h3[@class='listing-item__title']//a/@href",
        "//a[@class='article-item-title']/@href",
        "//a[@class='article-title' or @class='article-medium-title' or @class='article-small-title']/@href"

      ],
      "title_selector": "//h1/text()",
      "content_selectors": [
        "//div[@class='article-body' or @class='post-content__text' or @class='article-content']//p/text()",
        "//article//p/text()"
      ]
    },
     {
      "allowed_domains": "www.topky.sk",
      "start_urls": [
        "https://najrecept.topky.sk/najnovsie-recepty"
      ],
      "link_selector": [
        "//a[@class='-X']/@href"
      ],
      "title_selector": "//h1/text()",
      "content_selectors": [
        "//div[@class='instructions']//p/text()",
        "//p[contains(@class, 'perex')]/text()"
      ],
       "pagination_link": "//a[contains(@class,'link') and contains(@class,'right')]/@href",
       "page_limit": 5
    }
  ]
}